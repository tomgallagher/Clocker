{
    "runOnSave.statusMessageTimeout": 3000,
    "runOnSave.commands": [
        {
            "match": ".*\\.css$",
            "runIn": "terminal",
            "command": "npm run build",
        },
        {
            "match": ".*\\.js$",
            "runIn": "terminal",
            "command": "npm run build",
        }
    ],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
}